Static {
    width: 1fr;
    height: 100%;
    border: #f1937f;
}

.line {
    min-height: 3;
    max-height: 3;
    border: solid red;
    background: rgb(50,50,50)
}

.basecont {
    height: 100%;
    width: 1fr;
}

.module {
    min-height: 8;
    height: 40%;
    layout: vertical;
    width: 2fr;
    border: panel #53365d;
}

.modulesContainer > Static {
    margin-bottom: 1;
    padding: 1;
}

.modulesContainer {
    width: 100%;
    height: 100%;
    row-span: 2;
}